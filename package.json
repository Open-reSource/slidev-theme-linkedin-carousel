{
  "name": "slidev-theme-linkedin-carousel",
  "version": "0.1.1",
  "author": {
    "name": "Open {re}Source",
    "url": "https://github.com/Open-reSource"
  },
  "funding": "https://github.com/sponsors/Open-reSource",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "linkedin"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.8.1",
    "prism-theme-vars": "^0.2.5"
  },
  "devDependencies": {
    "@slidev/cli": "^51.8.0",
    "playwright-chromium": "^1.52.0"
  },
  "slidev": {
    "colorSchema": "light",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Poppins",
        "mono": "Fira Code"
      },
      "aspectRatio": "3/4",
      "canvasWidth": 800
    }
  },
  "files": [
    "assets",
    "components",
    "example.md",
    "example-export",
    "layouts",
    "setup",
    "styles"
